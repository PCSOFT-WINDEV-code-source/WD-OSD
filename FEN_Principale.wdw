     code : |1+
      // Résumé : Affiche le bouton permettant de fermer l'application ou de la déplacer
      // Syntaxe :
      //AfficheBouton ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // AfficheBouton()
      //// Procédure automatique :
      // La procédure est exécutée manuellement, lors d'un appel dans le code
      // Elle sera répétée en boucle, en attendant 10 centièmes de seconde entre chaque appel
      // Chaque appel suivant exécute une seule fois la procédure, sans timer
      // 
      
      PROCEDURE AfficheBouton()
      
      // Variables
      X, Y sont des entiers système
      
      // Initialisation
      X = PoidsFaible(CurseurPos(cpEcran))
      Y = PoidsFort(CurseurPos(cpEcran))
      
      SI MaFenêtre..X <= X <= MaFenêtre..Largeur + MaFenêtre..X _ET_ MaFenêtre..Y <= Y <= MaFenêtre..Hauteur + MaFenêtre..Y ALORS
      	SI PAS GR_FERMETUREGRIP..Visible ALORS
      		GR_FERMETUREGRIP..Visible = Vrai	
      	FIN
      SINON
      	SI GR_FERMETUREGRIP..Visible ALORS
      		GR_FERMETUREGRIP..Visible = Faux	
      	FIN 		
      FIN
     type : 458752
   -
     name : AfficheNiveauSon
     procedure_id : 1783680337306734915
     type_code : 14
     code : |1-
      // Résumé : Affiche le niveau de son courant du système
      // Syntaxe :
      //AfficheNiveauSon ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // AfficheNiveauSon()
      //
      Procédure AfficheNiveauSon()
      
      // Variable
      X       est un entier
      nValeur est un réel
      
      // Initialisation
      X = 0
      gnNiveauVolume = MCIVolume("WAV")
      
      // On calcule les rapports pour l'échelle de notre volume
      nValeur = 65535 / gnNbMaxNiveau
      gnNiveauVolume = gnNiveauVolume / nValeur
      
      dDébutDessin(IMG_Son, dEffacer)
      
      POUR I = 1 _A_ Abs(gnNiveauVolume)
      	dRectangle(X,0,X+15,IMG_Son..Hauteur,CyanClair)
      	X += 40
      FIN
     type : 458752
   -
     name : OnMove
     procedure_id : 1783756203575661234
     type_code : 14
     code : |1+
      // Résumé : Procédure appelée à chaque changement de position de la fenêtre
      // Syntaxe :
      //OnMove ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Evénement(OnMove, "*.*", WM_WINDOWPOSCHANGING )
      //
      Procédure OnMove()
      
      // Variables
      bMod		 est un booléen
      stPosFenetre est un STWINDOWPOS
      
      // Récupère les informations de déplacement
      Transfert(&stPosFenetre, _EVE.lParam, Dimension(stPosFenetre))
      
      // Si la gestion du magnétisme est active
      SI gbSansMagnet = Faux ALORS
      	// On magnétise sur le bord gauche de l'écran
      	SI MaFenêtre..X <= 50 ALORS	
      		stPosFenetre:x = 0
      		bMod = Vrai
      	FIN
      	
      	// On magnétise sur le bord haut de l'écran
      	SI MaFenêtre..Y <= 50 ALORS		
      		stPosFenetre:y = 0		
      		bMod = Vrai
      	FIN
      	
      	// On magnétise sur le bord droit de l'écran
      	SI MaFenêtre..X + MaFenêtre..Largeur >= SysXRes() - 50 ALORS		
      		stPosFenetre:x = SysXRes() - MaFenêtre..Largeur		
      		bMod = Vrai
      	FIN
      	
      	// On magnétise sur le bord bas de l'écran
      	SI MaFenêtre..Y + MaFenêtre..Hauteur >= SysYRes() - 50 ALORS	
      		stPosFenetre:y = SysYRes() - MaFenêtre..Hauteur	
      		bMod = Vrai
      	FIN
      	
      	// Si des modifications ont été apportées
      	SI bMod = Vrai ALORS 
      		// On retransmet les prochaines coordonnées
      		Transfert(_EVE.lParam, &stPosFenetre, Dimension(stPosFenetre))
      	FIN
      SINON
      	// Si on est en dehors des "bords" de l'écran, on peut reprendre la gestion du magnétisme
      	SI MaFenêtre..X > 50 _ET_ MaFenêtre..Y > 50 _ET_ MaFenêtre..X + MaFenêtre..Largeur < SysXRes() - 50 _ET_ MaFenêtre..Y + MaFenêtre..Hauteur < SysYRes() - 50 ALORS
      		gbSansMagnet = Faux
      	FIN
      FIN
     type : 458752
   -
     name : ButtonDown
     procedure_id : 1783757745469279498
     type_code : 14
     code : |1-
      // Résumé : Procédure appelée quand le bouton gauche est enfoncé et qui permet de vérifier la position de la fenêtre
      // et d'agir en conséquence pour le magnétisme.
      // Syntaxe :
      //ButtonDown ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // ButtonDown()
      //
      Procédure ButtonDown()
      
      gbSansMagnet = Faux
      
      SI MaFenêtre..X <= 50 ALORS
      	gbSansMagnet = Vrai
      FIN
      
      SI MaFenêtre..Y <= 50 ALORS
      	gbSansMagnet = Vrai
      FIN
      
      SI MaFenêtre..X + MaFenêtre..Largeur >= SysXRes() - 50 ALORS
      	gbSansMagnet = Vrai
      FIN
      
      SI MaFenêtre..Y + MaFenêtre..Hauteur >= SysYRes() - 50 ALORS
      	gbSansMagnet = Vrai
      FIN
