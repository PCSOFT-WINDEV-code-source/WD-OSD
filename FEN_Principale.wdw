     code : |1+
      
      // Constantes Windows Platform SDK
      CONSTANTE
      	GWL_EXSTYLE          = -20
      	WS_EX_LAYERED        = 0x00080000
      	LWA_COLORKEY         = 0x00000001
      	LWA_ALPHA            = 0x00000002
      	WM_WINDOWPOSCHANGING = 0x0046
      FIN
      
      // Structure renvoyée par le SDK Windows donnant des informations sur la fenêtre
      STWINDOWPOS est une structure
      	nHWnd est un entier
      	nHWndInsertAfter est un entier
      	x est un entier
      	y est un entier
      	cx est un entier
      	cy est un entier
      	nFlags est un entier sans signe	
      FIN
      
      // Niveau de son
      gnNiveauVolume est un réel
      gnNbMaxNiveau  est un entier
      
      // Magnétisme
      gbSansMagnet   est un booléen
      
      // Mise en place de l'événement pour gérer le déplacement de la fenêtre
      Evénement(OnMove, "*.*", WM_WINDOWPOSCHANGING)
      
   -
     internal_properties : CAAAAAgAAAAVPN2xeyrkj6JSM93Gzk6b2sxyYHEyrP1dzkxeCq2j0SThwQEA/z0z0s2Sr9H4XcwHK+07WDlGa8h1BYID+/Pcl+trD751k8WjnPRVj3kIry3i9wUPlXv+l5hRn4WheJIb17jCHKUl5Igq54DdYLLOZRSouKK/TD0e427HEo+amQ==
     code : |1-
      
      // On positionne la fenêtre
      MaFenêtre..X = (Val(ExtraitChaîne(SysRésolutionEcran(),1)) / 2) - (MaFenêtre..Largeur / 2)
      MaFenêtre..Y = (Val(ExtraitChaîne(SysRésolutionEcran(),2)) / 2) - (MaFenêtre..Hauteur / 2)
      
      // Initialisation
      gnNiveauVolume = MCIVolume("WAV")
      gnNbMaxNiveau  = Abs(IMG_Son..Largeur / 39) + 1
      
      // La fenêtre est toujours visible
      FenEtat(DessusTout)
      
      // Définit la couleur de fond comme Magenta (RVB(255,0,255))
      MaFenêtre..CouleurFond = RVB(255,0,255)
      
      // Changement du style de la fenêtre en WS_EX_LAYERED (fenêtre transparente)
      API("user32", "SetWindowLongA", Handle(),GWL_EXSTYLE, WS_EX_LAYERED)
      // On spécifie la couleur de transparence qui est magenta et on définit l'opacité (255)
      API("user32", "SetLayeredWindowAttributes", Handle(), RVB(255,0,255), 255, LWA_COLORKEY + LWA_ALPHA)	
      
      // Groupe de bouton pour déplacement & fermeture
      GR_FERMETUREGRIP..Visible = Faux
      
      // On lance les fonctions qui vont rafraîchir l'affichage
      TimerSon()
      AfficheBouton()
      
      // Fenêtre invisible au démarrage pour éviter un effet de clignotement pendant son initialisation
      MaFenêtre..Visible = Vrai
